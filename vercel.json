{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/favicon.ico", "dest": "/frontend/public/logo.ico" },
    { "src": "/assets/(.*)", "dest": "/frontend/dist/assets/$1" },
    { "src": "/[^.]+", "dest": "/frontend/dist/index.html", "status": 200 }
  ]
}
